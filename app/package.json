  "description": "The Tito Homerun pool, now in Angular 2.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run start-app",
    "build-app": "rimraf dist && webpack --progress --profile --bail",
    "start-app": "webpack-dev-server --progress --port 8080",
    "build": "rimraf dist && webpack --config config/build/webpack.stage.js --progress --profile --bail",
    "build-prod": "rimraf dist && webpack --config config/build/webpack.prod.js --progress --profile --bail",
    "postinstall": "typings install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
