{
  "name": "tito-node-api",
  "version": "1.0.0",
  "description": "tito api developed using node.js",
  "main": "api.js",
  "scripts": {
    "postinstall": "typings install && npm run mongo-init",
    "build": "rimraf dist && webpack --progress --profile --bail",
    "start": "concurrently \"npm run watch\" \"npm run mongo-start\" \"sleep 3 && npm run start-nodemon\"",
    "watch": "rimraf dist && webpack --watch --progress --profile --bail",
    "start-nodemon": "nodemon ./dist/api.js",
    "test": "NODE_ENV=spec mocha --opts mocha.opts && rimraf test",
    "test-watch": "NODE_ENV=spec mocha --watch --opts mocha.opts && rimraf test",
    "mongo-init": "mkdirp data/db",
    "mongo-start": "mongod --dbpath ./data/db"
  },
  "author": "Cheese",
  "license": "UNLICENSED",
  "homepage": "http://www.liquidelvis.com/tito/forms/login.jsp",
  "dependencies": {
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "config": "^1.24.0",
    "core-js": "^2.4.1",
    "express": "^4.14.0",
    "express-dependency-injector": "^2.0.4",
    "express-jwt": "^5.1.0",
    "jsonwebtoken": "^7.1.9",
    "mongoose": "^4.6.7",
    "oauth": "^0.9.14",
    "request": "^2.79.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "concurrently": "^3.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.1.2",
    "nodemon": "^1.11.0",
    "sinon": "^1.17.6",
    "sinon-as-promised": "^4.0.2",
    "sinon-chai": "^2.8.0",
    "ts-loader": "^0.9.5",
    "ts-node": "^1.7.0",
    "typings": "^2.0.0",
    "validator": "^6.1.0",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^0.15.0"
  }
}
